<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javasm.crm.aspect.mapper.LogDao">


	<!-- 保存日志记录 -->
	<insert id="insertSlog" parameterType="com.javasm.crm.aspect.entity.Slog">
		INSERT INTO syslogs(username,content,`time`) VALUES(#{username}, #{content},#{time})
	</insert>

</mapper>